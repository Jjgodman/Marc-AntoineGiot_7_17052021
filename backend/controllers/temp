    var formDT=new FormData(form)
    var forim=new FormData()
    forim.append('image',image)
    formDT.append('idUSERS',idUSERS)
    
    var donImg=Object.fromEntries(formDT)
    console.log(donImg);
    fetch ('http://localhost:3000/api/publi/addPubli',{
        method:'POST',
        headers:{
            "Content-Type" : "multipart/formdata; boundary=----WebKitFormBoundaryTEST",
        },
        formDT
    })
    .then(response => response.json())
    .then(data =>{
        console.log(data)
    })
    .catch(e=>{
        console.error(e)
    })
